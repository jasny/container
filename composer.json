{
    "name": "jasny/container",
    "type": "library",
    "description": "Simple dependency injection container compatible with container-interop",
    "keywords": [
        "dependency injection",
        "DI",
        "container-interop",
        "container",
        "PSR-11"
    ],
    "license": "MIT",
    "homepage": "http://jasny.github.com/container",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/container/issues",
        "source": "https://github.com/jasny/container"
    },
    "require": {
        "php": ">=7.1.0",
        "container-interop/container-interop": "~1.2",
        "psr/container": "^1.0",
        "jasny/php-functions": "^3.3"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3",
        "jasny/autowire": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Container\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpcloc cloc src",
            "phpcs -p src",
            "phpstan analyse"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all tests and quality checks"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
