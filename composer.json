{
    "name": "jasny/container",
    "type": "library",
    "description": "Simple dependency injection container with autowire support",
    "keywords": [
        "dependency injection",
        "container",
        "PSR-11"
    ],
    "license": "MIT",
    "homepage": "http://jasny.github.com/container",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/container/issues",
        "source": "https://github.com/jasny/container"
    },
    "require": {
        "php": ">=7.2.0",
        "ext-ctype": "*",
        "improved/type": "^0.1.1",
        "jasny/autowire": "dev-master as 1.2",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3",
        "bnf/phpstan-psr-container": "dev-patch-1 as 0.11.1"
    },
    "repositories": [
        { "type": "git", "url": "https://github.com/jasny/phpstan-psr-container" }
    ],
    "suggest": {
        "jasny/autowire": "Automatically determine the dependencies when instantiating a class"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Container\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit",
            "phpcs -p src"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all tests and quality checks"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
